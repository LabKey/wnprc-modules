  "betterScripts": {
    "build-prod": {
      "command": "webpack --config webpack/prod.config.js",
      "env": {
        "NODE_ENV": "production"
      }
    },
    "build-style": {
      "command": "webpack --config webpack/dev-style.config.js",
      "env": {
        "NODE_ENV": "development"
      }
    },
    "build-watch": {
      "command": "node webpack/dev-server.js",
      "env": {
        "NODE_ENV": "development"
      }
    },
    "clean": {
      "command": "rimraf resources/web/animalrequest/app/ ; rimraf node_modules"
    },
    "build-jest-test": {
      "command": "jest",
      "env": {
        "NODE_ENV": "test"
      }
    }
  }
}
