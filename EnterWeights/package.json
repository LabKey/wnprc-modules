{
  "name": "enterweights",
  "version": "1.0.0",
  "description": "ReactJS form for entering weights into LabKey EHR",
  "main": "index.js",
  "scripts": {
    "build": "better-npm-run build-prod",
    "build-prod": "better-npm-run build-prod",
    "build-dev": "better-npm-run build-dev",
    "start": "better-npm-run build:style && better-npm-run build:watch",
    "clean": "better-npm-run clean",
    "reallyClean": "better-npm-run reallyClean",
    "test": "better-npm-run build:jest-test",
    "analyze": "source-map-explorer resources/web/EnterWeights/app/app*"
  },
  "author": "csebranek",
  "license": "Apache 2.0",
  "dependencies": {
    "@types/react": "16.8.13",
    "@types/react-bootstrap": "0.32.20",
    "@types/react-dom": "15.5.1",
    "@types/react-router": "4.0.14",
    "@types/react-router-dom": "4.3.1",
    "@types/redux-actions": "1.2.7",
    "react": "16.8.1",
    "react-dom": "16.8.1",
    "@babel/polyfill": "7.4.0",
    "react-router-dom": "5.0.0",
    "react-router": "5.0.0",
    "connected-react-router": "6.3.2",
    "react-bootstrap": "1.0.0-beta.16",
    "react-datepicker": "2.13.0",
    "immutable": "^4.0.0-rc.12",
    "eslint": "^6.5.1",
    "react-select": "3.0.8",
    "@primer/octicons-react": "9.4.0",
    "dayjs": "1.8.21"
  },
  "devDependencies": {
    "@babel/core": "7.4.0",
    "@babel/preset-react": "7.0.0",
    "@babel/preset-typescript": "7.3.3",
    "@types/jest": "24.0.11",
    "babel-loader": "8.0.5",
    "babel-preset-es2015": "6.24.1",
    "better-npm-run": "0.0.15",
    "cors": "2.8.5",
    "css-loader": "0.28.7",
    "eslint-plugin-react-hooks": "2.3.0",
    "express": "4.16.4",
    "express-http-proxy": "1.5.1",
    "mini-css-extract-plugin": "0.9.0",
    "jest": "24.5.0",
    "jest-transform-css": "2.0.0",
    "prettier": "^1.19.1",
    "react-test-renderer": "16.8.1",
    "redux-devtools-extension": "2.13.2",
    "rimraf": "2.6.1",
    "style-loader": "0.18.2",
    "ts-jest": "24.0.0",
    "ts-loader": "4.5.0",
    "typedoc": "0.16.8",
    "typescript": "3.7.5",
    "webpack": "4.42.0",
    "webpack-cli": "3.3.11",
    "webpack-dev-middleware": "1.11.0",
    "webpack-hot-middleware": "2.18.2",
    "source-map-explorer": "2.3.1",
    "webpack-bundle-analyzer": "3.6.0"
  },
  "jest": {
    "globals": {},
    "transform": {
      "^.+\\.tsx?$": "ts-jest",
      ".+\\.(css|styl|less|sass|scss)$": "jest-transform-css"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.tsx?$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  },
  "betterScripts": {
    "build-prod": {
      "command": "webpack --config webpack/prod.config.js",
      "env": {
        "NODE_ENV": "production"
      }
    },
    "build-dev": {
      "command": "webpack --config webpack/dev.config.js",
      "env": {
        "NODE_ENV": "development"
      }
    },
    "build:style": {
      "command": "webpack --config webpack/dev-style.config.js",
      "env": {
        "NODE_ENV": "development"
      }
    },
    "build:watch": {
      "command": "node webpack/dev-server.js",
      "env": {
        "NODE_ENV": "development"
      }
    },
    "clean": {
      "command": "rimraf resources/web/enterweights/app/"
    },
    "reallyClean": {
      "command": "rimraf resources/web/enterweights/app/ ; rimraf node_modules"
    },
    "build:jest-test": {
      "command": "jest",
      "env": {
        "NODE_ENV": "test"
      }
    }
  }
}
