<query xmlns="http://labkey.org/data/xml/query">
    <metadata>
        <tables xmlns="http://labkey.org/data/xml">
            <table tableName="CurrentBehaviorNotes" tableDbType="NOT_IN_DB">
                <columns>
                    <column columnName="Id">
                        <isKeyField>true</isKeyField>
                        <isHidden>true</isHidden>
                    </column>
                    <column columnName="currentBehaviors">
                        <columnTitle>Current Behavior(s)</columnTitle>
                        <url>/query/executeQuery.view?schemaName=study&amp;
                            query.queryName=BehaviorAbstract&amp;
                            query.id~eq=${id}
                        </url>
                        <!--<formatString>#.##</formatString>-->
                    </column>
                    <column columnName="visitRowId">
                        <isHidden>true</isHidden>
                    </column>
                </columns>
                <titleColumn>currentBehaviors</titleColumn>
            </table>
        </tables>
    </metadata>
</query>
