<customView xmlns="http://labkey.org/data/xml/queryCustomView" label="All Problems">
    <columns>
        <column name="Id"/>
        <column name="Id/curLocation/room">
            <properties>
                <property name="columnTitle" value="Current Room"/>
            </properties>
        </column>
        <column name="Id/curLocation/cage">
            <properties>
                <property name="columnTitle" value="Current Cage"/>
            </properties>
        </column>
        <column name="problem_no"/>
        <column name="date"/>
        <column name="enddate"/>
        <column name="category"/>
        <column name="remark"/>
        <column name="code"/>
    </columns>
    <sorts>
        <sort column="date" descending="true"/>
    </sorts>
</customView>