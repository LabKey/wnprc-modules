<query xmlns="http://labkey.org/data/xml/query">
    <metadata>
        <tables xmlns="http://labkey.org/data/xml">
            <table tableName="mensesInterval" tableDbType="NOT_IN_DB">
                <columns>
                    <column columnName="lsid">
                        <isKeyField>true</isKeyField>
                        <isHidden>true</isHidden>
                    </column>
                    <column columnName="PrevDate">
                        <url>/query/executeQuery.view?schemaName=study&amp;
                            query.queryName=menses&amp;
                            query.id~eq=${id}&amp;
                            query.date~eq=${PrevDate}&amp;
                            query.sort=-date
                        </url>
                    </column>
                    <column columnName="Interval">
                        <columnTitle>Interval (Days)</columnTitle>
                    </column>
                </columns>
                <titleColumn>Interval</titleColumn>
            </table>
        </tables>
    </metadata>
</query>
