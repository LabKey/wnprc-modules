<customView xmlns="http://labkey.org/data/xml/queryCustomView">
  <columns>
      <column name="Id/curLocation/room">
        <properties>
            <property name="columnTitle" value="Current Room"/>
        </properties>
      </column>
      <column name="Id/curLocation/cage">
          <properties>
            <property name="columnTitle" value="Current Cage"/>
          </properties>
      </column>
      <column name="Id"/>
      <column name="date"/>
      <column name="userid"/>
      <column name="feces"/>
      <column name="menses"/>
      <column name="behavior"/>
      <column name="breeding"/>
      <column name="other"/>
      <column name="tlocation"/>
      <column name="remark"/>
      <column name="otherbehavior"/>
      <column name="qcstate" />
      <column name="taskid" />
  </columns>
   <sorts>
       <sort column="date" descending="true"/>
       <sort column="Id/curLocation/room"/>
       <sort column="Id/curLocation/cage"/>
   </sorts>
  <filters>
      <filter column="date" operator="dateeq" value="+0d" />
      <filter column="hasObs/hasObs" operator="eq" value="true"/>
      <!--<filter column="qcstate/publicdata" operator="eq" value="true"/>-->
  </filters>

</customView>