<query xmlns="http://labkey.org/data/xml/query">
    <metadata>
        <tables xmlns="http://labkey.org/data/xml">
            <table tableName="PregnancyInfo" tableDbType="NOT_IN_DB">
                <columns>
                    <column columnName="updatelink">
                        <columnTitle></columnTitle>
                        <url>/ehr/dataEntryForm.view?formType=Breeding%20Encounter&amp;taskid=${taskid}</url>
                        <urlTarget>_blank</urlTarget>
                    </column>
                    <column columnName="objectid">
                        <isHidden>true</isHidden>
                    </column>
                    <column columnName="taskid">
                        <isHidden>true</isHidden>
                    </column>
                    <column columnName="id">
                        <columnTitle>Dam</columnTitle>
                    </column>
                    <column columnName="sireid">
                        <columnTitle>Sire</columnTitle>
                        <url replaceMissing="blankValue">/ehr/participantView.view?participantId=${sireid}</url>
                        <fk>
                            <fkDbSchema>study</fkDbSchema>
                            <fkTable>animal</fkTable>
                            <fkColumnName>id</fkColumnName>
                        </fk>
                    </column>
                    <column columnName="date">
                        <columnTitle>Breeding Start</columnTitle>
                        <formatString>MM/dd/yyyy</formatString>
                    </column>
                    <column columnName="dateend">
                        <columnTitle>Breeding End</columnTitle>
                        <formatString>MM/dd/yyyy</formatString>
                    </column>
                    <column columnName="remark">
                        <columnTitle>Breeding Remark</columnTitle>
                    </column>
                    <column columnName="ejaculation">
                        <columnTitle>Ejaculation Confirmed?</columnTitle>
                    </column>
                    <column columnName="conceptiondate">
                        <columnTitle>Est. Conception</columnTitle>
                        <formatString>MM/dd/yyyy</formatString>
                    </column>
                    <column columnName="conceptiondate_plus_30">
                        <columnTitle>+ 30</columnTitle>
                        <formatString>MM/dd/yyyy</formatString>
                    </column>
                    <column columnName="conceptiondate_plus_60">
                        <columnTitle>+ 60</columnTitle>
                        <formatString>MM/dd/yyyy</formatString>
                    </column>
                    <column columnName="conceptiondate_plus_90">
                        <columnTitle>+ 90</columnTitle>
                        <formatString>MM/dd/yyyy</formatString>
                    </column>
                    <column columnName="conceptiondate_plus_120">
                        <columnTitle>+ 120</columnTitle>
                        <formatString>MM/dd/yyyy</formatString>
                    </column>
                    <column columnName="conceptiondate_plus_150">
                        <columnTitle>+ 150</columnTitle>
                        <formatString>MM/dd/yyyy</formatString>
                    </column>
                    <column columnName="conceptiondate_plus_165">
                        <columnTitle>+ 165</columnTitle>
                        <formatString>MM/dd/yyyy</formatString>
                    </column>
                    <column columnName="gestation_day">
                        <columnTitle>Current Gestation</columnTitle>
                        <formatString>0</formatString>
                    </column>
                    <column columnName="outcome">
                        <columnTitle>Outcome</columnTitle>
                    </column>
                    <column columnName="outcome_date">
                        <columnTitle>Outcome Date/Time</columnTitle>
                        <formatString>MM/dd/yyyy hh:mm</formatString>
                    </column>
                    <column columnName="outcome_remark">
                        <columnTitle>Outcome Remark</columnTitle>
                    </column>
                    <column columnName="infantid">
                        <columnTitle>Infant</columnTitle>
                        <url replaceMissing="blankValue">/ehr/participantView.view?participantId=${infantid}</url>
                        <fk>
                            <fkDbSchema>study</fkDbSchema>
                            <fkTable>animal</fkTable>
                            <fkColumnName>id</fkColumnName>
                        </fk>
                    </column>
                </columns>
            </table>
        </tables>
    </metadata>
</query>