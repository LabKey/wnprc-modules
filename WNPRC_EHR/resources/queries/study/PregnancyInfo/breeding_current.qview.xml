<customView xmlns="http://labkey.org/data/xml/queryCustomView" label="Breeding - Current" name="breeding_current">
    <columns>
        <column name="updatelink"/>
        <column name="id"/>
        <column name="sireid"/>
        <column name="date"/>
        <column name="dateend"/>
        <column name="reason"/>
        <column name="remark"/>
    </columns>
    <sorts>
        <sort column="date" descending="true"/>
    </sorts>
    <filters>
        <filter column="conceptiondate" operator="isblank"/>
        <filter column="outcome_date"   operator="isblank"/>
    </filters>
</customView>