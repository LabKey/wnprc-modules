<customView xmlns="http://labkey.org/data/xml/queryCustomView">
   <columns>
       <column name="Id"/>
       <column name="date"/>
       <column name="project"/>
       <column name="DataSet/Label">
            <properties>
                <property name="columnTitle" value="Dataset"/>
            </properties>
       </column>
       <column name="remark"/>
       <column name="description"/>
       <column name="performedby"/>
       <column name="qcstate"/>
   </columns>
   <sorts>
       <sort column="date" descending="true"/>
   </sorts>
   <filters>
       <filter column="dataset/name" operator="in" value="assignments;behavetrem;biopsy;blood;clinremarks;deaths;drug;encounters;housing;necropsy;problem;weight;tb;clinpathRuns;obs;Cage Observations"/>
       <!--<filter column="description"  operator="neq" value="No Irregular Observations"/>-->
       <!--<filter column="qcstate/publicdata" operator="eq" value="true"/>-->
   </filters>
</customView>