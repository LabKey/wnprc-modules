<query xmlns="http://labkey.org/data/xml/query">
    <description>Lookup geographic origin from arrival table but if its a birth its domestic</description>
    <metadata>
        <tables xmlns="http://labkey.org/data/xml">
            <table tableName="demographicsGeographicOrigin" tableDbType="NOT_IN_DB">
                <columns>
                    <column columnName="geographic_origin">
                        <fk>
                            <fkDbSchema>ehr_lookups</fkDbSchema>
                            <fkTable>geographic_origins</fkTable>
                            <fkColumnName>meaning</fkColumnName>
                        </fk>
                    </column>
                </columns>
            </table>
        </tables>
    </metadata>
</query>
