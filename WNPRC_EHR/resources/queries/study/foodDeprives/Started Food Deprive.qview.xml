<customView xmlns="http://labkey.org/data/xml/queryCustomView">
    <columns>
        <column name="Id" />
        <column name="date" />
        <column name="project"/>
        <column name="protocolContact"/>
        <column name="remarks"/>
        <column name="reason"/>
        <column name="schedule"/>
        <column name="depriveStartTime"/>
        <column name="restoredTime"/>
        <column name="qcstate"/>
        <column name="taskid"/>
        <column name="requestid"/>

    </columns>
    <sorts>
        <sort column="date" descending="true"/>
    </sorts>
    <filters>
        <filter column="QCState/Label" operator="eq" value="Started"/>
        <filter column="Id/dataset/demographics/calculated_status" operator="eq" value="Alive"/>
    </filters>
</customView>