                          <fkDbSchema>ehr</fkDbSchema>
                          <fkTable>tasks</fkTable>
                          <fkColumnName>taskid</fkColumnName>
                        </fk>
                        <url>/ehr/dataEntryFormDetails.view?formtype=Research%20Ultrasounds%20Task&amp;taskid=${taskid}</url>
                    </column>
                </columns>
            </table>
        </tables>
    </metadata>
</query>