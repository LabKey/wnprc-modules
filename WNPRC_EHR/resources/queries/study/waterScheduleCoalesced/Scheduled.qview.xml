<customView xmlns="http://labkey.org/data/xml/queryCustomView">
    <columns>
        <column name="Id" />
        <column name="dateOrdered"/>
        <column name="room"/>
        <column name="cage"/>
        <column name="volume" />
        <column name="frequency" />
        <column name="waterSource"/>
        <column name="dataSource"/>
        <column name="assignedTo"/>
        <column name="provideFruit"/>
        <column name="actionRequired"/>
        <column name="qcstate" />
        <column name="taskid" />
    </columns>
    <sorts>
        <sort column="date" descending="false"/>
        <sort column="room" />
        <sort column="cage" />
        <sort column="Id"/>
    </sorts>
    <filters>
    <filter column="qcstate/label" operator="eq" value="Scheduled"/>
    </filters>
</customView>