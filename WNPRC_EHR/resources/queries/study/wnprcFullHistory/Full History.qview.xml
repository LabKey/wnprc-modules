<customView xmlns="http://labkey.org/data/xml/queryCustomView">
   <columns>
       <column name="Id"/>
       <column name="date"/>
       <column name="project"/>
       <column name="dataset">
            <properties>
                <property name="columnTitle" value="Dataset"/>
            </properties>
       </column>
       <column name="remark"/>
       <column name="description"/>
       <column name="performedby"/>
       <column name="qcstate"/>
       <column name="taskid" />
       <column name="requestid" />
   </columns>
   <sorts>
       <sort column="date" descending="true"/>
   </sorts>
   <filters>
       <filter column="datasetname" operator="in" value="assignments;behavetrem;biopsy;blood;clinremarks;deaths;encounters;drug;housing;necropsy;problem;weight;tb;clinpathRuns;foodDeprives;ultrasounds;pregnancy_outcomes;breeding_encounters;research_ultrasounds,chairing,restraint;watertotal"/>
       <!--<filter column="qcstate/publicdata" operator="eq" value="true"/>-->
   </filters>
</customView>