<query xmlns="http://labkey.org/data/xml/query">
    <metadata>
        <tables xmlns="http://labkey.org/data/xml">
            <table tableName="mgap_sequence_datasets" tableDbType="TABLE">
                <javaCustomizer class="org.labkey.ehr.table.WNPRC_EHRCustomizer"/>
                <columns>
                    <column columnName="mgap_id">
                        <columnTitle>mGAP Id</columnTitle>
                    </column>
                    <column columnName="sequence_type">
                        <columnTitle>Sequence Type</columnTitle>
                    </column>
                    <column columnName="total_reads">
                        <columnTitle>Total Reads</columnTitle>
                        <datatype>integer</datatype>
                        <formatString>#,###</formatString>
                    </column>
                    <column columnName="sra_accession">
                        <columnTitle>SRA Accession</columnTitle>
                    </column>
                    <column columnName="original_id">
                        <columnTitle>Original Id</columnTitle>
                    </column>
                    <column columnName="parsed_id">
                        <columnTitle>Parsed Id</columnTitle>
                    </column>
                    <column columnName="container">
                        <columnTitle>Folder</columnTitle>
                        <isHidden>true</isHidden>
                    </column>
                    <column columnName="createdby">
                        <columnTitle>Created By</columnTitle>
                        <isHidden>true</isHidden>
                    </column>
                    <column columnName="created">
                        <columnTitle>Created</columnTitle>
                        <isHidden>true</isHidden>
                    </column>
                    <column columnName="modifiedby">
                        <columnTitle>Modified By</columnTitle>
                        <isHidden>true</isHidden>
                    </column>
                    <column columnName="modified">
                        <columnTitle>Modified</columnTitle>
                        <isHidden>true</isHidden>
                    </column>
                </columns>
            </table>
        </tables>
    </metadata>
</query>