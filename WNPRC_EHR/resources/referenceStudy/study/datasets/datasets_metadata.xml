<?xml version="1.0" encoding="UTF-8"?>
<tables xmlns="http://labkey.org/data/xml" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://labkey.org/data/xml ../../../../../../../schemas/tableInfo.xsd" >
    <sharedConfig>
        <columns>
            <column columnName="id">
                <datatype>varchar</datatype>
                <propertyURI>http://cpas.labkey.com/Study#ParticipantId</propertyURI>
                <nullable>false</nullable>
                <fk>
                    <fkDbSchema>study</fkDbSchema>
                    <fkTable>animal</fkTable>
                    <fkColumnName>id</fkColumnName>
                </fk>
            </column>
            <column columnName="objectid">
                <datatype>entityid</datatype>
                <isHidden>true</isHidden>
                <isKeyField>true</isKeyField>
                <propertyURI>urn:ehr.labkey.org/#ObjectId</propertyURI>
            </column>
            <column columnName="date">
                <datatype>timestamp</datatype>
                <propertyURI>http://cpas.labkey.com/Study#VisitDate</propertyURI>
            </column>
            <column columnName="requestid">
                <datatype>varchar</datatype>
                <propertyURI>urn:ehr.labkey.org/#RequestId</propertyURI>
            </column>
            <column columnName="taskid">
                <datatype>varchar</datatype>
                <propertyURI>urn:ehr.labkey.org/#TaskId</propertyURI>
            </column>
            <column columnName="performedby">
                <datatype>varchar</datatype>
                <propertyURI>urn:ehr.labkey.org/#PerformedBy</propertyURI>
            </column>
            <column columnName="description">
                <datatype>varchar</datatype>
                <propertyURI>urn:ehr.labkey.org/#Description</propertyURI>
            </column>
            <column columnName="remark">
                <datatype>varchar</datatype>
                <propertyURI>urn:ehr.labkey.org/#Remark</propertyURI>
            </column>
        </columns>
        <indices>
            <index type="unique">
                <column>objectid</column>
            </index>
            <index type="non-unique">
                <column>taskid</column>
            </index>
        </indices>
    </sharedConfig>
    <table tableName="breeding_encounters" tableDbType="TABLE">
        <tableTitle>Breeding Encounters</tableTitle>
        <description>Contains one row for each breeding pairing/assignment for a dam at the center.</description>
        <columns>
            <column columnName="dateend">
                <columnTitle>End Date</columnTitle>
                <datatype>timestamp</datatype>
            </column>
            <column columnName="sireid">
                <columnTitle>Sire</columnTitle>
                <datatype>varchar</datatype>
                <url replaceMissing="blankValue">/ehr/participantView.view?participantId=${sire_id}</url>
                <fk>
                    <fkDbSchema>study</fkDbSchema>
                    <fkTable>animal</fkTable>
                    <fkColumnName>id</fkColumnName>
                </fk>
            </column>
            <column columnName="reason">
                <columnTitle>Reason</columnTitle>
                <datatype>varchar</datatype>
            </column>
            <column columnName="conceptiondate">
                <columnTitle>Est. Conception Date</columnTitle>
                <datatype>timestamp</datatype>
            </column>
            <column columnName="ejaculation">
                <columnTitle>Ejaculation Confirmed</columnTitle>
                <datatype>varchar</datatype>
            </column>
        </columns>
    </table>
    <table tableName="breeding_remarks" tableDbType="TABLE">
        <tableTitle>Breeding Remarks</tableTitle>
        <description>Contains one row for each observation/remark about a breeding encounter.</description>
        <columns/>
    </table>
    <table tableName="pregnancy_outcomes" tableDbType="TABLE">
        <tableTitle>Pregnancy Outcomes</tableTitle>
        <description>Contains one row for the outcome of each pregnancy completed/terminated at the center.</description>
        <columns>
            <column columnName="outcome">
                <columnTitle>Outcome</columnTitle>
                <datatype>varchar</datatype>
                <nullable>false</nullable>
            </column>
            <column columnName="infantid">
                <columnTitle>Infant</columnTitle>
                <datatype>varchar</datatype>
                <url replaceMissing="blankValue">/ehr/participantView.view?participantId=${infant_id}</url>
                <fk>
                    <fkDbSchema>study</fkDbSchema>
                    <fkTable>animal</fkTable>
                    <fkColumnName>id</fkColumnName>
                </fk>
            </column>
        </columns>
    </table>
    <table tableName="ultrasounds" tableDbType="TABLE">
        <tableTitle>Ultrasounds</tableTitle>
        <description>Contains one row for each ultrasound performed at the center.</description>
        <columns/>
    </table>
    <table tableName="tissues_sample" tableDbType="TABLE">
        <columns>
            <column columnName="tissue">
                <columnTitle>Organ/Tissue</columnTitle>
                <datatype>string</datatype>
            </column>
            <column columnName="diagnosis">
                <columnTitle>Diagnosis</columnTitle>
                <datatype>string</datatype>
            </column>
            <column columnName="recipient">
                <columnTitle>Recipient</columnTitle>
                <datatype>string</datatype>
            </column>
            <column columnName="preservation">
                <columnTitle>Preservation</columnTitle>
                <datatype>string</datatype>
            </column>
            <column columnName="qualifier">
                <columnTitle>Qualifier</columnTitle>
                <datatype>string</datatype>
            </column>
            <column columnName="quantity">
                <columnTitle>Quantity</columnTitle>
                <datatype>string</datatype>
            </column>
            <column columnName="container_type">
                <columnTitle>Container Type</columnTitle>
                <datatype>string</datatype>
            </column>
            <column columnName="ship_to">
                <columnTitle>Ship To</columnTitle>
                <datatype>string</datatype>
            </column>
            <column columnName="accountToCharge">
                <columnTitle>Account To Charge</columnTitle>
                <datatype>string</datatype>
            </column>
            <column columnName="tissueRemarks">
                <columnTitle>Tissue Remark</columnTitle>
                <datatype>string</datatype>
            </column>
            <column columnName="slideNum">
                <columnTitle>Cassette Number</columnTitle>
                <datatype>string</datatype>
            </column>
            <column columnName="stain">
                <columnTitle>Stain</columnTitle>
                <datatype>string</datatype>
            </column>
            <column columnName="pathologist">
                <columnTitle>Pathologist</columnTitle>
                <datatype>string</datatype>
            </column>
            <column columnName="trimdate">
                <columnTitle>Trim date </columnTitle>
                <datatype>timestamp</datatype>
            </column>
            <column columnName="trimmed_by">
                <columnTitle>Trimmed By</columnTitle>
                <datatype>string</datatype>
            </column>
            <column columnName="trim_remarks">
                <columnTitle>Trim Remarks</columnTitle>
                <datatype>string</datatype>
            </column>
            <column columnName="ship_to_comment">
                <columnTitle>Ship To Comment</columnTitle>
                <datatype>string</datatype>
            </column>
            <column columnName="lab_sample_id">
                <columnTitle>Lab Sample ID</columnTitle>
                <datatype>string</datatype>
            </column>
            <column columnName="collect_before_death">
                <columnTitle>Collect Before Death</columnTitle>
                <datatype>boolean</datatype>
                <description>If true, the tissue should be collected antemortem.</description>
            </column>
            <column columnName="collection_order">
                <columnTitle>Collection Order</columnTitle>
                <datatype>integer</datatype>
                <description>This is the order that tissues should appear in the collection list.</description>
            </column>
        </columns>
    </table>
</tables>