<folderType xmlns="http://labkey.org/data/xml/folderType">
    <name>WNPRC_Virology</name>
    <description>Base folder type for virology services shared data folders.</description>
    <requiredWebParts>
        <webPart>
            <name>bannerInfo</name>
            <location>body</location>
        </webPart>
    </requiredWebParts>
    <preferredWebParts>
        <webPart>
            <name>FAQ</name>
            <location>menu</location>
        </webPart>
    </preferredWebParts>
    <modules>
        <moduleName>Core</moduleName>
        <moduleName>Study</moduleName>
    </modules>
    <defaultModule>Core</defaultModule>
    <workbookType>false</workbookType>
    <menubarEnabled>true</menubarEnabled>
</folderType>