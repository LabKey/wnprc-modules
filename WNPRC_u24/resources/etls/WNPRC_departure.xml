<?xml version="1.0" encoding="UTF-8"?>
<etl xmlns="http://labkey.org/etl/xml">
    <name>arrivals</name>
    <description>WNPRC Marmoset Departures</description>
    <transforms>
        <transform type="RemoteQueryTransformStep" id="step1">
            <description>Copy to target</description>
            <source remoteSource="ProductionEHRServer" schemaName="study" queryName="departure">
                <sourceColumns>
                    <column>id</column>
                    <column>date</column>
                    <column>destination</column>
                </sourceColumns>
                <sourceFilters>
                    <sourceFilter column="species/scientific_name" value="Callithrix jacchus" operator="eq"/>
                </sourceFilters>
            </source>
            <destination schemaName="study" queryName="u24_departure" targetOption="merge" />
        </transform>
    </transforms>
    <schedule>
        <cron expression="0 30 1 * * ?" />
    </schedule>
</etl>
