<query xmlns="http://labkey.org/data/xml/query">
    <metadata>
        <tables xmlns="http://labkey.org/data/xml">
            <table tableName="pregnancy_info" tableDbType="NOT_IN_DB">
                <columns>
                    <column columnName="objectid">
                        <isHidden>true</isHidden>
                    </column>
                    <column columnName="estimated_conception_date">
                        <columnTitle>Est. Conception</columnTitle>
                        <formatString>MM/dd/yyyy</formatString>
                    </column>
                    <column columnName="estimated_conception_date_plus_30">
                        <columnTitle>+ 30</columnTitle>
                        <formatString>MM/dd/yyyy</formatString>
                    </column>
                    <column columnName="estimated_conception_date_plus_60">
                        <columnTitle>+ 60</columnTitle>
                        <formatString>MM/dd/yyyy</formatString>
                    </column>
                    <column columnName="estimated_conception_date_plus_90">
                        <columnTitle>+ 90</columnTitle>
                        <formatString>MM/dd/yyyy</formatString>
                    </column>
                    <column columnName="estimated_conception_date_plus_120">
                        <columnTitle>+ 120</columnTitle>
                        <formatString>MM/dd/yyyy</formatString>
                    </column>
                    <column columnName="estimated_conception_date_plus_150">
                        <columnTitle>+ 150</columnTitle>
                        <formatString>MM/dd/yyyy</formatString>
                    </column>
                    <column columnName="estimated_conception_date_plus_165">
                        <columnTitle>+ 165</columnTitle>
                        <formatString>MM/dd/yyyy</formatString>
                    </column>
                    <column columnName="breeding_start_date">
                        <formatString>MM/dd/yyyy</formatString>
                    </column>
                    <column columnName="breeding_end_date">
                        <formatString>MM/dd/yyyy</formatString>
                    </column>
                    <column columnName="outcome_date">
                        <formatString>MM/dd/yyyy hh:mm</formatString>
                    </column>
                    <column columnName="breeding_comment">
                        <columnTitle>Breeding Comment</columnTitle>
                    </column>
                    <column columnName="ejaculation_confirmation">
                        <columnTitle>Ejaculation Confirmed?</columnTitle>
                    </column>
                    <column columnName="outcome_comment">
                        <columnTitle>Outcome Comment</columnTitle>
                    </column>
                    <column columnName="Id">
                        <columnTitle>Dam</columnTitle>
                    </column>
                    <column columnName="sire_id">
                        <columnTitle>Sire</columnTitle>
                        <url replaceMissing="blankValue">/ehr/participantView.view?participantId=${sire_id}</url>
                        <fk>
                            <fkDbSchema>study</fkDbSchema>
                            <fkTable>Animal</fkTable>
                            <fkColumnName>Id</fkColumnName>
                        </fk>
                    </column>
                    <column columnName="infant_id">
                        <columnTitle>Infant</columnTitle>
                        <url replaceMissing="blankValue">/ehr/participantView.view?participantId=${infant_id}</url>
                        <fk>
                            <fkDbSchema>study</fkDbSchema>
                            <fkTable>Animal</fkTable>
                            <fkColumnName>Id</fkColumnName>
                        </fk>
                    </column>
                    <column columnName="is_current">
                        <isHidden>true</isHidden>
                    </column>
                </columns>
            </table>
        </tables>
    </metadata>
</query>