<customView xmlns="http://labkey.org/data/xml/queryCustomView" label="Current Pregnancies">
    <columns>
        <column name="Id"/>
        <column name="sire_id"/>
        <column name="breeding_start_date"/>
        <column name="breeding_end_date"/>
        <column name="breeding_reason"/>
        <column name="estimated_conception_date"/>
        <column name="estimated_conception_date_plus_30"/>
        <column name="estimated_conception_date_plus_60"/>
        <column name="estimated_conception_date_plus_90"/>
        <column name="estimated_conception_date_plus_120"/>
        <column name="estimated_conception_date_plus_150"/>
        <column name="estimated_conception_date_plus_165"/>
    </columns>
    <sorts>
        <sort column="estimated_conception_date" descending="true"/>
    </sorts>
    <filters>
        <filter column="outcome_date" operator="isblank"/>
    </filters>
</customView>