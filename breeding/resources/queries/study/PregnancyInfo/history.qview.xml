<customView xmlns="http://labkey.org/data/xml/queryCustomView" label="Pregnancy History" name="history">
    <columns>
        <column name="Id"/>
        <column name="sire_id"/>
        <column name="breeding_start_date"/>
        <column name="breeding_end_date"/>
        <column name="breeding_comment"/>
        <column name="outcome"/>
        <column name="outcome_date"/>
        <column name="infant_id"/>
    </columns>
    <sorts>
        <sort column="outcome_date" descending="true"/>
    </sorts>
    <filters>
        <filter column="outcome_date" operator="isnonblank"/>
    </filters>
</customView>