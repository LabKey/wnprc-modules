#!/bin/bash

FB_PREFIX="21.11w_fb_"

if [[ $SOURCE_BRANCH == $FB_PREFIX* ]];
then
        SHORT_BRANCH_NAME=${SOURCE_BRANCH#"$FB_PREFIX"}
else
        SHORT_BRANCH_NAME="master"
fi

docker build --build-arg LABKEY_TEAMCITY_USERNAME=$teamcityUser --build-arg LABKEY_TEAMCITY_PASSWORD=$teamCityPWD --build-arg WNPRC_BRANCH=$SHORT_BRANCH_NAME --rm=true -t $IMAGE_NAME .
