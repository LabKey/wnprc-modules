<customView xmlns="http://labkey.org/data/xml/queryCustomView">
    <columns>
        <column name="Id"/>
        <column name="Id/Dataset/Demographics/gender">
            <properties>
                <property name="columnTitle" value="Gender"/>
            </properties>
        </column>

        <column name="Id2"/>
        <column name="Id2/gender_code">
            <properties>
                <property name="columnTitle" value="Gender 2"/>
            </properties>
        </column>
        <column name="coefficient" />
    </columns>
   <sorts>
       <sort column="Id"/>
       <sort column="coefficient" descending="true"/>
   </sorts>
</customView>