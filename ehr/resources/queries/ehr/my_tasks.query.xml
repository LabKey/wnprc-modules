                        <nullable>true</nullable>
                        <fk>
                            <fkDbSchema>core</fkDbSchema>
                            <fkTable>PrincipalsWithoutAdmin</fkTable>
                            <fkColumnName>userid</fkColumnName>
                        </fk>
                    </column>
                    <column columnName="duedate">
