<query xmlns="http://labkey.org/data/xml/query">
    <metadata>
        <tables xmlns="http://labkey.org/data/xml">
            <table tableName="projectLastAssignmentDate" tableDbType="NOT_IN_DB">
                <columns>
                    <column columnName="project">
                        <isKeyField>true</isKeyField>
                    </column>
                    <column columnName="lastAssignmentDate">
                        <url>/query/executeQuery.view?schemaName=study&amp;
                            query.queryName=Assignment&amp;
                            query.project~eq=${project}&amp;
                            query.sort=-Date
                        </url>
                    </column>
                </columns>
                <titleColumn>LastAssignmentDate</titleColumn>
            </table>
        </tables>
    </metadata>
</query>
