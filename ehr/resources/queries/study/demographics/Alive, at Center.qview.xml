<customView xmlns="http://labkey.org/data/xml/queryCustomView">
    <columns>
        <column name="Id"/>
        <column name="Id/curLocation/room">
            <properties>
                <property name="columnTitle" value="Room"/>
            </properties>
        </column>
        <column name="Id/curLocation/cage">
            <properties>
                <property name="columnTitle" value="Cage"/>
            </properties>
        </column>
        <column name="species"/>
        <column name="gender">
            <!--<properties>-->
                <!--<property name="columnTitle" value="Gender"/>-->
            <!--</properties>-->
        </column>
        <!--<column name="Status">-->
            <!--<properties>-->
                <!--<property name="columnTitle" value="Status"/>-->
            <!--</properties>-->
        <!--</column>-->
        <column name="calculated_status">
            <properties>
                <property name="columnTitle" value="Status"/>
            </properties>
        </column>
        <column name="Id/activeAssignments/Availability">
            <properties>
                <property name="columnTitle" value="Avail"/>
            </properties>
        </column>
        <column name="Id/activeAssignments/AssignmentStatus">
            <properties>
                <property name="columnTitle" value="Assignment Status"/>
            </properties>
        </column>
        <column name="hold"/>
        <column name="dam"/>
        <column name="sire"/>
        <column name="origin"/>
        <column name="birth"/>
        <column name="death"/>
        <!--<column name="Id/MostRecentArrival/MostRecentArrival">-->
            <!--<properties>-->
                <!--<property name="columnTitle" value="Most Recent Arrival"/>-->
            <!--</properties>-->
        <!--</column>-->
        <!--<column name="Id/MostRecentDeparture/MostRecentDeparture">-->
            <!--<properties>-->
                <!--<property name="columnTitle" value="Most Recent Departure"/>-->
            <!--</properties>-->
        <!--</column>-->
        <column name="Id/curLocation/cond">
            <properties>
                <property name="columnTitle" value="Condition"/>
            </properties>
        </column>
        <!--<column name="Id/numRoommates/AnimalsInCage">-->
            <!--<properties>-->
                <!--<property name="columnTitle" value="# Animals In Cage"/>-->
            <!--</properties>-->
        <!--</column>-->
        <column name="Id/MostRecentWeight/MostRecentWeight">
            <properties>
                <property name="columnTitle" value="Current Weight (kg)"/>
            </properties>
        </column>
        <column name="Id/MostRecentWeight/MostRecentWeightDate">
            <properties>
                <property name="columnTitle" value="Weight Date"/>
            </properties>
        </column>
        <column name="Id/MostRecentTBDate/MostRecentTBDate">
            <properties>
                <property name="columnTitle" value="Most Recent TB Date"/>
            </properties>
        </column>
        <column name="medical"/>
        <column name="prepaid"/>
        <column name="history"/>
    </columns>
    <filters>
        <filter column="calculated_status" operator="eq" value="Alive"/>
    </filters>
    <sorts>
        <sort column="Id/curLocation/room"/>
        <sort column="Id/curLocation/cage"/>
    </sorts>
</customView>