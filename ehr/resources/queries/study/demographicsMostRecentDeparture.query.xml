<query xmlns="http://labkey.org/data/xml/query">
    <metadata>
        <tables xmlns="http://labkey.org/data/xml">
            <table tableName="demographicsMostRecentDeparture" tableDbType="NOT_IN_DB">
                <columns>
                    <column columnName="Id">
                        <isKeyField>true</isKeyField>
                        <isHidden>true</isHidden>
                    </column>
                    <column columnName="MostRecentDeparture">
                        <url>/query/executeQuery.view?schemaName=study&amp;
                            query.queryName=departure&amp;
                            query.Id~eq=${Id}&amp;
                            query.sort=-Date&amp;
                        </url>
                    </column>
                </columns>
                <titleColumn>MostRecentDeparture</titleColumn>
            </table>
        </tables>
    </metadata>
</query>
