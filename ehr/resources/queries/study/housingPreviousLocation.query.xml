<query xmlns="http://labkey.org/data/xml/query">
    <metadata>
        <tables xmlns="http://labkey.org/data/xml">
            <table tableName="housingPreviousLocation" tableDbType="TABLE">
                <columns>
                    <column columnName="lsid">
                        <isHidden>true</isHidden>
                    </column>
                    <column columnName="location">
                        <columnTitle>Previous Location</columnTitle>
                        <displayWidth>120</displayWidth>
                    </column>
                    <column columnName="room">
                        <columnTitle>Previous Room</columnTitle>
                    </column>
                    <column columnName="cage">
                        <columnTitle>Previous Cage</columnTitle>
                    </column>
                </columns>
            </table>
        </tables>
    </metadata>
</query>