<query xmlns="http://labkey.org/data/xml/query">
    <metadata>
        <tables xmlns="http://labkey.org/data/xml">
            <table tableName="housingTotalRoommates" tableDbType="NOT_IN_DB">
                <tableTitle>Total Roommates Per Housing Record</tableTitle>
                <columns>
                    <column columnName="lsid">
                        <isKeyField>true</isKeyField>
                        <isHidden>true</isHidden>
                    </column>
                    <column columnName="TotalRoommates">
                        <url>/query/executeQuery.view?schemaName=study&amp;
                            query.queryName=housingRoommates&amp;
                            query.lsid~eq=${lsid}&amp;
                            query.sort=Id&amp;
                        </url>
                        <columnTitle>Total Cagemates</columnTitle>
                    </column>
                </columns>
                <titleColumn>TotalRoommates</titleColumn>
            </table>
        </tables>
    </metadata>
</query>
