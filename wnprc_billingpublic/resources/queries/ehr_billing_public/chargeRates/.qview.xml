<customView xmlns="http://labkey.org/data/xml/queryCustomView" label="All Rates">
    <columns>
        <column name="chargeId"/>
        <column name="unitCost"/>
        <column name="startDate"/>
        <column name="endDate"/>
        <column name="serviceCode"/>
        <column name="description"/>
        <column name="genCredits"/>
        <column name="groupName"/>
    </columns>
    <sorts>
        <sort column="chargeId/name"/>
        <sort column="startDate" descending="true"/>
    </sorts>
</customView>